# Copy to .env and adjust if needed. .env is gitignored.
# Used by docker-compose for Postgres and Grafana.

# Host port for Postgres (default 54320 to avoid conflict with local Postgres on 5432)
POSTGRES_PORT=54320
POSTGRES_USER=grid
POSTGRES_PASSWORD=grid-secret
POSTGRES_DB=grid

# Grafana admin (default: admin/admin)
GRAFANA_ADMIN_PASSWORD=admin
